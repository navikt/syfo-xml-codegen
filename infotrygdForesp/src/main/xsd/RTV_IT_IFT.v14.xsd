							<xs:restriction base="xs:string">
								<xs:enumeration value="U"/>
								<xs:enumeration value="O"/>
								<xs:enumeration value="H"/>
								<xs:enumeration value="P"/>
								<xs:enumeration value="S"/>
								<xs:enumeration value="A"/>
								<xs:enumeration value="K"/>
								<xs:enumeration value="R"/>
								<xs:enumeration value="V"/>
								<xs:enumeration value="D"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="utredetEllerOperertDato" type="xs:date" use="optional"/>
					<xs:attribute name="del2Type" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>J eller U</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="del2Dato" type="xs:date" use="optional"/>
					<xs:attribute name="friskmeldtDato" type="xs:date" use="optional"/>
					<xs:attribute name="svangerskapSykdom" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>J eller ingenting</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="skadeart" type="xs:string" use="optional"/>
					<xs:attribute name="skadedato" type="xs:date" use="optional"/>
					<xs:attribute name="skademeldMottatt" type="xs:date" use="optional"/>
					<xs:attribute name="skadeVetak" type="xs:string" use="optional"/>
					<xs:attribute name="unntakAktivitet" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>M = Medisinske årsaker
A = Arbeidsrelatert aktivitet på arbeidsplassen kan ikke gjennomføres</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="oppfolgingsplan" type="xs:date" use="optional"/>
					<xs:attribute name="avventendeSykmelding" type="xs:string" use="optional"/>
					<xs:attribute name="avventendeSykmeldingTOM" type="xs:date" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Historikk" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:choice minOccurs="0">
						<xs:element name="Uforegrad">
							<xs:complexType>
								<xs:attribute name="fom" type="xs:date" use="optional"/>
								<xs:attribute name="tom" type="xs:date" use="optional"/>
								<xs:attribute name="grad" type="xs:integer" use="optional"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Lege">
							<xs:complexType>
								<xs:sequence>
									<xs:choice minOccurs="0">
										<xs:element name="Legenummer" type="xs:integer"/>
										<xs:element name="Institusjonsnummer" type="xs:integer"/>
									</xs:choice>
									<xs:element name="Navn" type="typeNavn" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Hvis institusjon, legg alt i fornavndelen av typeNavn</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="kodeLegeInstitusjon" type="xs:string" use="optional"/>
								<xs:attribute name="legeFodselsnummer" type="typeFodselsnummer" use="optional">
									<xs:annotation>
										<xs:documentation>Kan også være institusjonsnr</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="fom" type="xs:date" use="optional"/>
								<xs:attribute name="tom" type="xs:date" use="optional"/>
								<xs:attribute name="legenrinstnr" type="xs:string" use="optional"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="HovedDiagnose">
							<xs:complexType>
								<xs:attribute name="kode" type="xs:string" use="optional"/>
								<xs:attribute name="kodeverk" type="xs:string" use="optional"/>
								<xs:attribute name="navn" type="xs:string" use="optional"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="SMDel2">
							<xs:annotation>
								<xs:documentation>Dette er Sm del 2 dato</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="registreringsDato" type="xs:date" use="optional"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Unntak">
							<xs:complexType>
								<xs:attribute name="kode" type="xs:string" use="optional"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="BiDiagnose">
							<xs:complexType>
								<xs:attribute name="kode" type="xs:string" use="optional"/>
								<xs:attribute name="kodeverk" type="xs:string" use="optional"/>
								<xs:attribute name="navn" type="xs:string" use="optional"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Helsetjenester">
							<xs:complexType>
								<xs:attribute name="kjoepHelsetjeneste" type="xs:string" use="optional"/>
								<xs:attribute name="helsetjenesteSendt" type="xs:date" use="optional"/>
								<xs:attribute name="utredetEllerOperert" type="xs:string" use="optional"/>
								<xs:attribute name="utredetEllerOperertDato" type="xs:date" use="optional"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Tilltak">
							<xs:annotation>
								<xs:documentation>Er ikke historik, men det kan  være mange.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="type" type="xs:string" use="required"/>
								<xs:attribute name="fom" type="xs:date" use="required"/>
								<xs:attribute name="tom" type="xs:date" use="required"/>
								<xs:attribute name="forlengetDato" type="xs:date" use="optional"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Svangerskap">
							<xs:annotation>
								<xs:documentation>Er historikk</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="kode" type="xs:string" use="required"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="Legeerklaring">
							<xs:complexType>
								<xs:attribute name="mottatDato" type="xs:date" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:choice>
					<xs:attribute name="endringsDato" type="xs:date" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typeYtelsePeriode">
		<xs:annotation>
			<xs:documentation>Periode for parallelle ytelser</xs:documentation>
		</xs:annotation>
		<xs:attribute name="fraDato" type="xs:date" use="required"/>
		<xs:attribute name="tilDato" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation>TOM er ikke required - den finnes bl.a. ikke  for BarnSyk.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="grad" type="xs:integer" use="required"/>
		<xs:attribute name="stoenadsType" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:whiteSpace value="preserve"/>
					<xs:enumeration value="AP"/>
					<xs:enumeration value="OM"/>
					<xs:enumeration value="FP"/>
					<xs:enumeration value="OP"/>
					<xs:enumeration value="PP"/>
					<xs:enumeration value="SV"/>
					<xs:enumeration value="PB"/>
					<xs:enumeration value="Z "/>
					<xs:enumeration value="PI"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="stans" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Avsluttet-frisk = AF 
Flyttet = FL
Avsluttet = AA
Avsluttet-død = AD
MaxUtbet = MAX
Sm2 mangler = SM-II
Død = DØD
Midlertidig stans = MIDL
Stans dato = 19870301 - 99999999
Opphør arbeidsforhold = OA
Bortfall omsorgsansvar = BO
Gravid adopsjon = GA
Spesielle forhold = SF
SP under aktivisering = SA
SP under akt uten fgodkj = IA
Friskmeldt til arbfor = FA
Reisetilskott = RT
Yrkesrettet attføring = YA
Bortfall omsorgsansvar = BO
GravidAdopsjon = GA</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="frisk" type="xs:string" use="optional"/>
		<xs:attribute name="utbetTOM" type="xs:date" use="optional"/>
	</xs:complexType>
	<xs:complexType name="typeFraAetat">
		<xs:annotation>
			<xs:documentation>Dataene kommer fra Aetat - attføring og dagpenger</xs:documentation>
		</xs:annotation>
		<xs:attribute name="tkNummer" type="xs:string" use="required"/>
		<xs:attribute name="arbeidskontorNummer" type="xs:string" use="required"/>
		<xs:attribute name="typeYtelse" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Verdier:
DP - dagpenger
AT - attføringspenger</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="datoFOM" type="xs:date" use="required"/>
		<xs:attribute name="datoTOM" type="xs:date" use="required"/>
		<xs:attribute name="typeData" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>F - foreløpig vedtak
V - vedtak
U - utbetaling
E - etterbetaling</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="gradering" type="xs:nonNegativeInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="statusType">
		<xs:attribute name="returStatus" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation>Alvorlighetsgrad.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="kodeMelding" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Meldingskode</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="beskMelding" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Meldingstekst</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="tidsStempel" type="typeTidsStempel" use="required">
			<xs:annotation>
				<xs:documentation>Tidsstempel når IT legger melding på returkø</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="typeYrkesskadeVedtak">
		<xs:attribute name="vedtaksDato" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation>Vedtaks dato fra saken YS-vedtaket ble fattet</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="diagnoseKode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Diagnose-kode (4 tegn)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="diagnoseTekst" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Beskrivende tekst for diagnosen (maks. 60 tegn)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="skadeArt" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Beskrivelse av skadeart (maks. 60 tegn)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="tknr" type="xs:string" use="optional"/>
	</xs:complexType>
</xs:schema>
